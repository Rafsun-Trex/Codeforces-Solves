#include<stdio.h>
int main()
{
    int i, n, a, b, c=0;
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        scanf("%d %d",&a, &b);
        if(a%b != 0)
        {
            c = b - (a%b);
            printf("%d\n", c);
        }
        else
        {
            printf("0\n");
        }
    }
    return 0;
}
