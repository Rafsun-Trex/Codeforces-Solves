#include<stdio.h>
int main()
{
    int n, x,y,count1=0,count2=0,i;
    scanf("%d",&n);
    for(i=1; i<=n; i++)
    {
        scanf("%d %d",&x,&y);
        if(x>y)
        {
            count1++;
        }
        else if(x<y)
        {
            count2++;
        }
        else
        {
            continue;
        }
    }
    if(count1>count2)
    {
        printf("Mishka\n");
    }
    else if(count2>count1)
    {
        printf("Chris\n");
    }
    else
    {
        printf("Friendship is magic!^^\n");
    }
}
