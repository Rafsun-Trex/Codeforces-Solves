#include<stdio.h>
int main()
{
    int n, n1,n2,n3, x1=0, x2=0, x3=0, i;
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        scanf("%d%d%d",&n1,&n2,&n3);
        x1+=n1;
        x2+=n2;
        x3+=n3;
    }
    if(x1==0&&x2==0&&x3==0)
    {
        printf("YES\n");
    }
    else
    {
        printf("NO\n");
    }
}
