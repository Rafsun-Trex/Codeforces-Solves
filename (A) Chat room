#include<stdio.h>
#include<string.h>
int main()
{
    char x[100];
    int n,i,sum=0,count=0;
    gets(x);
    n = strlen(x);
    for(i=0;i<n;i++)
    {
        if(x[i]=='h')
        {
            sum++;
            count=i;
            break;
        }
    }
    for(i=0;i<n;i++)
    {
        if(x[i]=='e')
        {
            if(i>count)
            {
                sum++;
                count=i;
                break;
            }
        }
    }
    for(i=0;i<n;i++)
    {
        if(x[i]=='l')
        {
            if(i>count)
            {
                sum++;
                count=i;
                break;
            }
        }
    }
    for(i=0;i<n;i++)
    {
        if(x[i]=='l')
        {
            if(i>count)
            {
                sum++;
                count=i;
                break;
            }
        }
    }
    for(i=0;i<n;i++)
    {
        if(x[i]=='o')
        {
            if(i>count)
            {
                sum++;
                count=i;
                break;
            }
        }
    }
    if(sum==5)
    {
        printf("YES\n");
    }
    else
    {
        printf("NO\n");
    }

}
