#include<stdio.h>
int main()
{
    int n,x,len[1001],i,sum=0;
    scanf("%d %d",&n,&x);
    for(i=0;i<n;i++)
    {
        scanf("%d",&len[i]);
        if(len[i]<=x)
        {
            sum = sum + 1;
        }
        else
        {
            sum = sum + 2;
        }
    }
    printf("%d\n",sum);
}
